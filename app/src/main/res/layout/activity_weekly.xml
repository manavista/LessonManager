        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/activity_weekly_row_horizontal_margin"
        android:layout_marginTop="@dimen/activity_weekly_row_horizontal_margin"
        android:layout_weight="1"
        android:background="@color/lightGray">

        <include
            layout="@layout/content_timetable"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

        <include
